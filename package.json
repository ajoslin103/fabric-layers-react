{
  "name": "fabric-layers",
  "version": "1.0.0",
  "description": "A fabric.js coordinate-plane (grid) & layers library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "browser": {
    "./dist/index.cjs.js": "./dist/index.umd.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --require @babel/register",
    "build": "npm run clean && rollup -c",
    "build:watch": "rollup -c -w",
    "start": "webpack serve --mode=development --open",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoslin103/fabric-layers.git"
  },
  "keywords": [
    "fabric.js",
    "canvas",
    "grid",
    "coordinate-plane",
    "layers",
    "axis",
    "polar",
    "cartesian"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajoslin103/fabric-layers/issues"
  },
  "homepage": "https://github.com/ajoslin103/fabric-layers#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "acorn": "^8.11.3",
    "chai": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.13.2",
    "rollup-plugin-terser": "^7.0.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "eventemitter2": "^6.4.9",
    "fabric-pure-browser": "^3.4.0"
  },
  "peerDependencies": {
    "fabric-pure-browser": "^3.4.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
