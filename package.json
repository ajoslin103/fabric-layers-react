{
  "name": "fabric-layers-react",
  "version": "2.0.0",
  "type": "module",
  "description": "A fabric.js coordinate-plane (grid) & layers library for React",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "docs": "node ./build/docs.js",
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --watch --config=jest.config.json",
    "test:coverage": "jest --coverage --config=jest.config.json",
    "start": "webpack-dev-server --mode development --open",
    "build": "rollup -c build/rollup-config.js",
    "build:cjs": "rollup -c build/rollup-config.js --environment BUILD:cjs",
    "build:esm": "rollup -c build/rollup-config.js --environment BUILD:esm",
    "build:umd": "rollup -c build/rollup-config.js --environment BUILD:umd",
    "release": "npm run test && npm run build && npm publish",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "rollup": "rollup -c build/rollup-config.js",
    "lib": "webpack",
    "watch": "rollup -w -c build/rollup-watch-config.js",
    "uglify": "uglifyjs dist/index.umd.js -c -m -o dist/index.umd.min.js --source-map filename=dist/index.umd.min.js.map --source-map-url index.umd.js.map --comments",
    "integrity": "node ./build/integrity.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoslin103/fabric-layers-react.git"
  },
  "keywords": [
    "fabric.js",
    "canvas",
    "grid",
    "coordinate-plane",
    "layers",
    "react",
    "cartesian",
    "polar"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajoslin103/fabric-layers-react/issues"
  },
  "homepage": "https://github.com/ajoslin103/fabric-layers-react#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "acorn": "^8.11.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "git-rev-sync": "^3.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.41.1",
    "rollup-plugin-git-version": "^0.3.1",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "eventemitter2": "^6.4.9",
    "fabric-layers": "file:../fabric-layers",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.13.0"
  },
  "peerDependencies": {
    "fabric-pure-browser": "^5.1.0",
    "react": "^19.1.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
